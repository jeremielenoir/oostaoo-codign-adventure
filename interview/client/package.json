  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --ext .jsx src/",
    "lint:fix": "eslint --ext .jsx --fix src/",
    "eject": "react-scripts eject",
    "watch": "npm-watch start"
