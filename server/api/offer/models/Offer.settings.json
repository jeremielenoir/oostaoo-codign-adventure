{
  "connection": "default",
  "collectionName": "offers",
  "info": {
    "name": "offer",
    "description": "tarif, title, desciption, periodicity"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "price": {
      "default": "",
      "type": "decimal"
    },
    "title": {
      "default": "",
      "type": "text"
    },
    "description": {
      "default": "",
      "type": "text"
    },
    "periodicity": {
      "default": "",
      "type": "string"
    },
    "nom": {
      "default": "",
      "type": "string"
    },
    "plan": {
      "default": "",
      "type": "string"
    },
    "payment_id": {
      "collection": "payment",
      "via": "offer_id"
    },
    "tests_stock": {
      "default": "",
      "type": "integer"
    },
    "user_id": {
      "collection": "user",
      "via": "offer_id",
      "plugin": "users-permissions"
    }
  }
}