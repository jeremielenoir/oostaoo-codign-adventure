{
  "connection": "default",
  "collectionName": "offers",
  "info": {
    "name": "offer",
    "description": "tarif, title, desciption, periodicity"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "price": {
      "default": "",
      "type": "decimal"
    },
    "title": {
      "default": "",
      "type": "text"
    },
    "description": {
      "default": "",
      "type": "text"
    },
    "nom": {
      "default": "",
      "type": "string"
    },
    "plan": {
      "default": "",
      "type": "string"
    },
    "tests_stock": {
      "default": "",
      "type": "integer"
    },
    "users": {
      "collection": "user",
      "via": "offer_id",
      "plugin": "users-permissions"
    },
    "payments": {
      "collection": "payment",
      "via": "offer"
    },
    "periodicity": {
      "default": "",
      "type": "string"
    },
    "enabled": {
      "default": true,
      "type": "boolean",
      "required": true
    }
  }
}